{
  "name": "template-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "android": "expo run:android",
    "build": "npx expo run:ios && npx expo run:android",
    "coverage": "npx jest --coverage",
    "e2e-android": "detox test --configuration android.emu.debug",
    "e2e-first": "detox build --configuration ios.sim.debug && detox build --configuration android.emu.debug",
    "e2e-ios": "detox test --configuration ios.sim.debug",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "reset-project": "node ./scripts/reset-project.js",
    "start": "expo start --dev-client",
    "test:watch": "jest --watch",
    "test": "jest",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@testing-library/react-native": "^13.2.0",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "expo": "~54.0.23",
    "husky": "^9.1.7",
    "i18next": "^25.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "eslint-config-prettier": "^10.1.5",
    "jest-expo": "~54.0.13",
    "jest": "~29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "detox": "^20.40.0",
    "eslint-config-expo": "~10.0.0",
    "eslint": "^9.25.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
